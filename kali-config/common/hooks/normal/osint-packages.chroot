cargo install -f --path .
export PATH=""/root/.cargo/bin:$PATH""
source ~/.bashrc
chmod +x /usr/bin/sn0int

#pip install -U checkdmarc
#pip install -U git+https://github.com/domainaware/checkdmarc.git

